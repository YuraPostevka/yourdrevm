
@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    <li><a href=@Url.Action("Logout", "Home")><span class="glyphicon glyphicon glyphicon-log-out"></span></a></li>
    <li>@Html.ActionLink("Invite", "InviteUser", "Home")</li>
}
else
{
    <li>@Html.ActionLink("Login", "Login", "Home")</li>
    <li><a href=@Url.Action("Login", "Home")><span class="glyphicon glyphicon glyphicon-log-in"></span></a></li>
}
